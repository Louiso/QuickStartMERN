{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage" : "https://louiso.github.io/QuickStartMERN/",
  "dependencies": {
    "antd": "^3.18.2",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dot-env": "^0.0.1",
    "express": "^4.17.3",
    "mongoose": "^5.5.11",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "dev:client": "react-scripts start",
    "dev:server": "eval $(cat .env) nodemon bin/www --watch server",
    "test:client": "react-scripts test",
    "eject": "react-scripts eject",
    "build:client": "react-scripts build",
    "build:server": "babel server -d dist-server/",
    "clean": "rm -rf build-app/* && rmdir build-app",
    "build": "yarn clean && yarn build:client && yarn build:server && mkdir build-app && mv build/ build-app/dist-client && mv dist-server/ build-app/dist-server",
    "start": "yarn build && eval $(cat .env.production) node build-app/dist-server"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eval": "^0.1.3",
    "nodemon": "^1.19.0"
  }
}
